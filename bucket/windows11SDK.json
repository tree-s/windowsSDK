        "args": [
            "/uninstall",
            "/quiet",
            "/norestart"
        ]
    },
    "checkver": {
        "url": "https://developer.microsoft.com/en-us/windows/downloads/sdk-archive",
        "regex": "Windows SDK for Windows 11[^(]*\\(([^\\)]+)[^?]*[^=]*=([0-9]*)",
        "replace": "${1}"
    },
    "autoupdate": {
        "url": "https://go.microsoft.com/fwlink/?linkid=$match2#/winsdksetup.exe"
    }
